class user:
    def __init__(self, size, price, quantity):
        self.size = size
        self.price = price
        self.quantiy = quantity

    def calculate_price(self):
        if self.size == "Small":
            self.price += 10
        elif self.size == "Medium":
            self.price += 12
        elif self.size == "Large":
            self.price += 15
        elif self.size == "X Large":
            self.price += 18
